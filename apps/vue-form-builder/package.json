{
  "name": "@myparcel/vue-form-builder",
  "version": "1.0.0-beta.1",
  "keywords": [
    "vue",
    "form-builder",
    "form",
    "vue3"
  ],
  "repository": "github:myparcelnl/vue-form-builder",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./src": "./src/index.ts"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist",
    "./src"
  ],
  "scripts": {
    "build": "run ws:tsup:build \"$(pwd)\"",
    "build:dev": "run ws:tsup:build:dev \"$(pwd)\"",
    "clean": "run ws:clean \"$(pwd)\"",
    "serve": "run ws:tsup:serve \"$(pwd)\"",
    "typecheck": "run ws:tsup:typecheck \"$(pwd)\"",
    "watch": "run ws:tsup:watch \"$(pwd)\""
  },
  "dependencies": {
    "@myparcel-vfb/core": "workspace:^1.0.0-beta.1",
    "@myparcel-vfb/hook-manager": "workspace:^1.0.0-beta.1",
    "@myparcel-vfb/plugin": "workspace:^1.0.0-beta.1",
    "@myparcel-vfb/utils": "workspace:^1.0.0-beta.1",
    "@myparcel/ts-utils": "^1.6.0"
  },
  "devDependencies": {
    "@myparcel-vfb/build-tsup": "workspace:*",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vue": "^3.2.47",
    "vue-tsc": "^1.1.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
