{
  "name": "@myparcel/vue-form-builder",
  "version": "1.0.0-beta.28",
  "keywords": [
    "vue",
    "form-builder",
    "form",
    "vue3"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.cjs",
        "default": "./dist/index.cjs"
      }
    },
    "./ts": {
      "types": "./dist/index.d.ts",
      "import": "./src/index.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "./dist",
    "./src"
  ],
  "scripts": {
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "clean": "run ws:clean \"$(pwd)\"",
    "typecheck": "run ws:ts:typecheck \"$(pwd)\""
  },
  "dependencies": {
    "@myparcel-vfb/core": "workspace:^1.0.0-beta.27",
    "@myparcel-vfb/hook-manager": "workspace:^1.0.0-beta.9",
    "@myparcel-vfb/plugin": "workspace:^1.0.0-beta.27",
    "@myparcel-vfb/utils": "workspace:^1.0.0-beta.5",
    "@myparcel/ts-utils": "^1.6.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-custom-tsconfig": "^1.0.4",
    "vite-plugin-dts": "^2.1.0",
    "vitest": "^1.0.0",
    "vue": "^3.2.47",
    "vue-tsc": "^1.6.5"
  },
  "peerDependencies": {
    "vue": "^3"
  },
  "publishConfig": {
    "access": "public"
  }
}
