{
  "name": "@myparcel-vfb/hook-manager",
  "version": "1.0.0-beta.2",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./src": "./src/index.ts"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist",
    "./src"
  ],
  "scripts": {
