{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "defaultBase": "alpha",
  "affected": {
    "defaultBase": "alpha"
  },
  "defaultProject": "@myparcel/vue-form-builder",
  "npmScope": "myparcel-vfb",
  "namedInputs": {
    "default": [
      "{workspaceRoot}/tsconfig.build.json",
      "{workspaceRoot}/yarn.lock",
      "{workspaceRoot}/package.json",
      "{projectRoot}/**/*"
    ],
    "prod": [
      "default",
      "!{projectRoot}/**/*.spec.*",
      "!{projectRoot}/**/__tests__/**",
      "!{projectRoot}/**/__mocks__/**",
      "!{projectRoot}/**/__snapshots__/**"
    ],
    "test": [
      "default",
      "{projectRoot}/**/*.spec.*",
      "{projectRoot}/**/__tests__/**",
      "{projectRoot}/**/__mocks__/**",
      "{projectRoot}/**/__snapshots__/**"
    ]
  },
  "targetDefaults": {
    "build:dev": {
      "inputs": ["prod"],
      "dependsOn": ["^build:dev"],
      "outputs": ["{projectRoot}/dist", "{projectRoot}/*.tsbuildinfo"]
    },
    "build": {
      "inputs": ["prod"],
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/dist", "{projectRoot}/*.tsbuildinfo"]
    },
    "serve": {
      "dependsOn": ["^build:dev"],
      "outputs": ["{projectRoot}/dist"]
    },
    "test:run": {
      "inputs": ["test"]
    },
    "test:coverage": {
      "inputs": ["test"],
      "outputs": ["{workspaceRoot}/coverage"]
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "parallel": 10,
        "cacheableOperations": [
          "build",
          "build:dev",
          "test:coverage",
          "test:run",
          "typecheck"
        ]
      }
    },
    "cloud": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "parallel": 10,
        "cacheableOperations": [
          "build",
          "test:coverage",
          "test:run",
          "typecheck"
        ],
        "accessToken": "ODE1MmI4YjctMmFiZC00YzNlLTg5YjUtMGIzNjgyMjI3NjhkfHJlYWQtd3JpdGU="
      }
    }
  }
}
