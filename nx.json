{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "defaultBase": "main",
  "defaultProject": "@myparcel/vue-form-builder",
  "npmScope": "myparcel-vfb",
  "namedInputs": {
    "default": ["{projectRoot}/lib"],
    "src": [
      "{projectRoot}/src",
      "{projectRoot}/package.json",
      "{projectRoot}/tsconfig.json"
    ]
  },
  "targetDefaults": {
    "build:dev": {
      "dependsOn": ["^build:dev"],
      "outputs": ["{projectRoot}/lib"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/lib"]
    },
    "buildOnly": {
      "dependsOn": ["^buildOnly"],
      "outputs": ["{projectRoot}/lib"]
    },
    "serve": {
      "dependsOn": ["build:dev"],
      "outputs": ["{projectRoot}/lib"]
    },
    "test": {
      "inputs": ["{workspaceRoot}/packages/**/*"],
      "outputs": ["{workspaceRoot}/coverage"]
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "parallel": 10,
        "cacheableOperations": [
          "build",
          "build:dev",
          "buildOnly",
          "test",
          "typecheck"
        ]
      }
    }
  }
}
