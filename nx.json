{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "buildOnly",
          "test"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "buildOnly": {
      "dependsOn": [
        "^buildOnly"
      ]
    },
    "dev": {
      "dependsOn": [
        "^dev"
      ]
    }
  },
  "defaultBase": "main"
}
