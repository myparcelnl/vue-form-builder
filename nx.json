{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "defaultBase": "main",
  "defaultProject": "@myparcel/vue-form-builder",
  "npmScope": "myparcel-vfb",
  "targetDefaults": {
    "build:dev": {
      "dependsOn": ["^build:dev"],
      "outputs": ["{projectRoot}/lib"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/lib"]
    },
    "serve": {
      "dependsOn": ["build:dev"],
      "outputs": ["{projectRoot}/lib"]
    },
    "test": {
      "outputs": ["{workspaceRoot}/coverage"]
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "parallel": 10,
        "cacheableOperations": ["build", "build:dev", "test", "typecheck"],
        "accessToken": "ODE1MmI4YjctMmFiZC00YzNlLTg5YjUtMGIzNjgyMjI3NjhkfHJlYWQtd3JpdGU="
      }
    }
  }
}
