{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "defaultBase": "alpha",
  "affected": {
    "defaultBase": "alpha"
  },
  "defaultProject": "@myparcel/vue-form-builder-js",
  "npmScope": "myparcel-vfb",
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "sharedGlobals"],
    "sharedGlobals": [
      "{workspaceRoot}/tsconfig.build.json",
      "{workspaceRoot}/yarn.lock",
      "{workspaceRoot}/package.json"
    ]
  },
  "targetDefaults": {
    "build:dev": {
      "inputs": ["default"],
      "dependsOn": ["^build:dev"],
      "outputs": ["{projectRoot}/dist", "{projectRoot}/*.tsbuildinfo"]
    },
    "build": {
      "inputs": ["default"],
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/dist", "{projectRoot}/*.tsbuildinfo"]
    },
    "serve": {
      "dependsOn": ["^build:dev"],
      "outputs": ["{projectRoot}/dist"]
    },
    "test:run": {
      "outputs": ["{workspaceRoot}/coverage"]
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "parallel": 10,
        "cacheableOperations": ["build", "build:dev", "test", "typecheck"]
      }
    },
    "cloud": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "parallel": 10,
        "cacheableOperations": ["build", "build:dev", "test", "typecheck"],
        "accessToken": "ODE1MmI4YjctMmFiZC00YzNlLTg5YjUtMGIzNjgyMjI3NjhkfHJlYWQtd3JpdGU="
      }
    }
  }
}
