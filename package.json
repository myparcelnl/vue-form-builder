{
  "name": "@myparcel/vue-form-builder-monorepo",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "lib/vue-form-builder.js",
  "types": "lib/src/index.d.ts",
  "workspaces": [
    "packages/**"
  ],
  "scripts": {
    "build": "nx run-many --target=build",
    "buildOnly": "nx run-many --target=buildOnly",
    "clean": "nx run-many --target=clean",
    "dev": "nx run-many --target=dev",
    "test": "nx run-many --target=test",
    "typecheck": "nx run-many --target=typecheck"
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint --fix",
