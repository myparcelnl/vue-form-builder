{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "compilerOptions": {
    "declaration": true,
    "importsNotUsedAsValues": "remove",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "preserveValueImports": false,
    "target": "ESNext",
    "types": ["vite/client", "node"]
  },
  "include": [],
  "exclude": [
    "**/*.snap",
    "**/*.spec.ts",
    "**/*.test.ts",
    "**/__tests__/**",
    "**/dist",
    "**/lib"
  ]
}
